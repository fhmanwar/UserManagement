
@{
    Layout = null;
}

<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Not Found ~ User Management</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="~/icon.ico" type="image/x-icon" />

    <!-- Fonts and icons -->
    <script src="~/js/Plugin/webfont.min.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Open+Sans:300,400,600,700"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands"], urls: ['/css/fonts.min.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link href="~/css/animated.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/css/Style.css">

</head>
<body>

    <div class="page-not-found">
        <div class="wrapper not-found">
            <h1 class="animated fadeIn">404</h1>
            <div class="desc animated fadeIn"><span>OOPS!</span><br />Looks like you get lost</div>
            <a href="/" class="btn btn-primary btn-back-home mt-4 animated fadeInUp">
                <span class="btn-label mr-2">
                    <i class="flaticon-home"></i>
                </span>
                Back To Home
            </a>
        </div>
    </div>


    <script src="~/js/Plugin/jquery.3.2.1.min.js"></script>
    <script src="~/js/Plugin/jquery-ui.min.js"></script>
    <script src="~/js/Plugin/popper.min.js"></script>
    <script src="~/js/Plugin/bootstrap.min.js"></script>

    <!-- jQuery UI -->
    <script src="~/js/Plugin/jquery.ui.touch-punch.min.js"></script>

    <!-- Bootstrap Notify -->
    <script src="~/js/Plugin/bootstrap-notify.min.js"></script>

    <!-- Bootstrap Toggle -->
    <script src="~/js/Plugin/bootstrap-toggle.min.js"></script>

</body>
</html>